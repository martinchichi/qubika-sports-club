{
    "compilerOptions": {
      "target": "ES2022",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "*": ["*"]
      },
      "types": ["node", "playwright"]
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }